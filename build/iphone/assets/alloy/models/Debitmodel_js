var Alloy=require("alloy"),_=require("alloy/underscore")._,model,collection;exports.definition={config:{columns:{col1:"TEXT",col2:"TEXT",col3:"TEXT",col4:"TEXT",col5:"TEXT",col6:"TEXT",col7:"TEXT",col8:"TEXT",col9:"TEXT"},adapter:{type:"sql",collection_name:"debitmodel"}},extendModel:function(e){return _.extend(e.prototype,{}),e},extendCollection:function(e){return _.extend(e.prototype,{deleteAll:function(){var e=this,t="DELETE FROM "+e.config.adapter.collection_name;console.log("collection deleteAll: "+t),db=Ti.Database.open(e.config.adapter.db_name),db.execute(t),db.close(),e.trigger("sync")},deleteCol1:function(e){var t=this,i="DELETE FROM "+t.config.adapter.collection_name+' WHERE col1="'+e+'"';db=Ti.Database.open(t.config.adapter.db_name),db.execute(i),db.close(),t.trigger("sync")},saveAll:function(){var e=this,t=e.config.adapter.db_name,i=e.config.adapter.collection_name,o=e.config.columns;db=Ti.Database.open(t),db.execute("BEGIN;"),e.each(function(e){e.id||(e.id=guid(),e.attributes[e.idAttribute]=e.id);var t=[],a=[],n=[];for(var r in o)t.push(r),a.push(e.get(r)),n.push("?");var l="INSERT INTO "+i+" ("+t.join(",")+") VALUES ("+n.join(",")+");";db.execute(l,a)}),db.execute("COMMIT;"),db.close(),e.trigger("sync")}}),e}},model=Alloy.M("debitmodel",exports.definition,[]),collection=Alloy.C("debitmodel",exports.definition,model),exports.Model=model,exports.Collection=collection;