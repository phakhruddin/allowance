var Alloy=require("alloy"),_=require("alloy/underscore")._,model,collection;exports.definition={config:{columns:{col1:"TEXT",col2:"TEXT",col3:"TEXT",col4:"TEXT",col5:"TEXT",col6:"TEXT",col7:"TEXT",col8:"TEXT",col9:"TEXT"},adapter:{type:"sql",collection_name:"creditmodel"}},extendModel:function(t){return _.extend(t.prototype,{}),t},extendCollection:function(t){return _.extend(t.prototype,{deleteAll:function(){var t=this,e="DELETE FROM "+t.config.adapter.collection_name;console.log("collection deleteAll: "+e),db=Ti.Database.open(t.config.adapter.db_name),db.execute(e),db.close(),t.trigger("sync")},deleteCol1:function(t){var e=this,i="DELETE FROM "+e.config.adapter.collection_name+' WHERE col1="'+t+'"';db=Ti.Database.open(e.config.adapter.db_name),db.execute(i),db.close(),e.trigger("sync")},saveAll:function(){var t=this,e=t.config.adapter.db_name,i=t.config.adapter.collection_name,o=t.config.columns;db=Ti.Database.open(e),db.execute("BEGIN;"),t.each(function(t){t.id||(t.id=guid(),t.attributes[t.idAttribute]=t.id);var e=[],a=[],r=[];for(var n in o)e.push(n),a.push(t.get(n)),r.push("?");var l="INSERT INTO "+i+" ("+e.join(",")+") VALUES ("+r.join(",")+");";db.execute(l,a)}),db.execute("COMMIT;"),db.close(),t.trigger("sync")}}),t}},model=Alloy.M("creditmodel",exports.definition,[]),collection=Alloy.C("creditmodel",exports.definition,model),exports.Model=model,exports.Collection=collection;